opam-version: "1"
maintainer: "kakadu.hafanana@gmail.com"
build: [
  ["./configure"]
  ["make" "generator"]
  ["sh" "-c" "cd lablqml && ./configure --datarootdir=%{lib}% && %{make}%" ]
  [make "opam.install" ]
]
remove: [
  ["ocamlfind" "remove" "lablqml"]
  ["rm" "-f" "%{prefix}%/bin/mocml" "%{man}%/man1/mocml.1"]
]
depends: ["ppx_sexp_conv" "core_kernel" "yojson" "conf-qt" {>= "5.2" } ]

